				$("#recursoLido label").text("Recurso lido");
			} else {
				$("#proximoRecurso").attr("data-bs-toggle", "modal").attr("data-bs-target", "#recursoNaoLido");
				$("#recursoLido label").text("Marcar recurso como lido");
			}
		});

		$(".exibir-mais").on("click", function(e) {
